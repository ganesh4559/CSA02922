#include <stdio.h>
int main ()
{
    char str[100]="master minds",ch;
    int count=0;
    printf(" enter the character to find its freqency =");
    scanf ("%c",&ch);
    for(int i=0;str[i]!='\0';++i)
    {
       if (ch==str[i])
       ++count;
    }
    printf("freqency of %c=%d",ch,count);
    return 0;
}c
