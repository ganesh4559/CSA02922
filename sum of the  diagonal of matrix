
#include <stdio.h>
int main() 
{
    int A[3][3]={1,2,3,4,5,6,7,8,9};
    int i,j,sum=0;
    for (i=0;i<3;i++)
    {
        for (j=0;j<3;j++)
        {
            printf("%d \t",A[i][j]);
        }
        printf("\n");
    }
    for(i=0;i<3;i++)
    {
        for (j=0;j<3;j++)
        {
            if(i==j)
            {
                printf("diagonal elements =%d",A[i][j]);
            }
        }
    }
    for(i=0;i<3;i++)
    {
        sum+=A[i][i];
    }
    printf("sum of diagonal elements is =%d",sum);
    return 0;
}
